// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User{
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  email      String @unique
  name       String 
  image      String
  excersises String[]
}

model Exercises{
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  ownerID   String
  name      String @unique
  history   Historic[]
}

type Historic{


}